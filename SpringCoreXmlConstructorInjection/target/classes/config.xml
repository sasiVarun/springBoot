<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="securityMode" class="com.test.SecurityMode">
		<constructor-arg>
			<value>ACTIVE</value>
		</constructor-arg>
		<constructor-arg>
			<value>10</value>
		</constructor-arg>
		<constructor-arg>
			<value>LOGIN</value>
		</constructor-arg>
	</bean>
</beans>
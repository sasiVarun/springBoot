<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="pob" class="in.cg.beans.Product">
		<property name="pid">
			<value>101</value>
		</property>
		<property name="pcode">
			<value>Crayons</value>
		</property>
		<property name="colors">
			<set>
				<value>Red</value>
				<value>Blue</value>
				<value>Pink</value>
				<value>Black</value>
				<value>Green</value>
			</set>
		</property>
		<property name="pdimensions">
			<list>
				<value>10</value>
				<value>20</value>
				<value>34</value>
				<value>10</value>
			</list>
		</property>

	</bean>
</beans>